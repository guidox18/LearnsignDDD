<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <LastUsedBuildConfiguration>Release</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://modokeke.gear.host</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>publish.gear.host</MSDeployServiceURL>
    <DeployIisAppPath>modokeke</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>True</EnableMSDeployBackup>
    <UserName>$modokeke</UserName>
    <_SavePWD>True</_SavePWD>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="HRConexion" Order="1" Enabled="False">
          <Destination Path="" />
          <Object Type="DbDacFx">
            <PreSource Path="User Id=HR;Password=oracle;Data Source=dsOracle" includeData="False" />
            <Source Path="$(IntermediateOutputPath)AutoScripts\HRConexion_IncrementalSchemaOnly.dacpac" dacpacAction="Deploy" />
          </Object>
          <UpdateFrom Type="Web.Config">
            <Source MatchValue="User Id=HR;Password=oracle;Data Source=dsOracle" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
          </UpdateFrom>
        </ObjectGroup>
        <ObjectGroup Name="HRSolucion.Datos.EF.Contextos.ContextoBase`1" Order="3" Enabled="False">
          <Destination Path="" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="HRSolucion.Datos.EF.Contextos.ContextoBase`1, HRSolucion.Datos.EF" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="HRcnx" Order="4" Enabled="False">
          <Destination Path="Data Source=mssql2.gear.host;Initial Catalog=keke;Integrated Security=False;User ID=keke;Password=KEKE.2015;Connect Timeout=15;Encrypt=False;TrustServerCertificate=False" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="HRSolucion.Datos.EF.Contextos.HRContexto, HRSolucion.Datos.EF" Origin="Configuration" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)HRcnx-Web.config Connection String">
      <ParameterValue>Data Source=mssql2.gear.host;Initial Catalog=keke;Integrated Security=False;User ID=keke;Password=KEKE.2015;Connect Timeout=15;Encrypt=False;TrustServerCertificate=False</ParameterValue>
      <UpdateDestWebConfig>False</UpdateDestWebConfig>
    </MSDeployParameterValue>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)HRConexion-Web.config Connection String" />
    <MSDeployParameterValue Include="$(DeployParameterPrefix)HRSolucion.Datos.EF.Contextos.ContextoBase`1-Web.config Connection String">
      <UpdateDestWebConfig>False</UpdateDestWebConfig>
    </MSDeployParameterValue>
  </ItemGroup>
  <ItemGroup>
    <_ConnectionStringsToInsert Include="HRSolucion.Datos.EF.Contextos.ContextoBase`1" />
  </ItemGroup>
</Project>